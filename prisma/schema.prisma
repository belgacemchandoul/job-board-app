generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Job {
  id          String    @id @default(cuid())
  title       String
  description String
  missions    Json[]
  status      Boolean?
  profiles    Json[]
}